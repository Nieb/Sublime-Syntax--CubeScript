          scope: constant.numeric.integer.hexadecimal.cfg

        #  Float:
        - match: ((((?<![a-zA-Z\d])\d*)|((?<![a-zA-Z\d])-\d*))(((?<![a-zA-Z\d])-\d{1}\.{1})|((?<![\d])-\.{1}\d{1})|(\d{1}\.{1})|(\.{1}\d{1})){1}\d*)
          scope: constant.numeric.float.cfg

        #  Integer:
        - match: ((\b((?![a-zA-Z])\d*)\b)|((?<![a-zA-Z\d])-\d{1}\d*))
          scope: constant.numeric.integer.cfg


        #####################################################################################
        #  Operators:
